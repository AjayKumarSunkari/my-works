# Service registers under this name
# Control the InternalResourceViewResolver:

spring.application.name=carz4u-web
spring.mvc.view.prefix=/WEB-INF/views/
spring.mvc.view.suffix=.jsp

#Logging Settings
logging.path=D:/Ajay/workspace/Carz4U_SpringBoot MicroService_V6.0/Log Files/
logging.file=${logging.path}Carz4U.log
logging.level.root=WARN
logging.level.org.springframework.web=WARN
logging.level.com.springboot.carz4u.*=INFO


# Discovery Server Access
eureka.client.serviceUrl.defaultZone=http://localhost:1111/eureka/

# Disable Spring Boot's "Whitelabel" default error page, so we can use our own
error.whitelabel.enabled=false    

#Disabling the Default Whitelabel Error Page
server.error.whitelabel.enabled=false
      
